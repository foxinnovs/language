{
    "name": "foxinnovs/language",
    "description": "Laravel Language Package - Fork compatible Laravel 12",
    "keywords": ["laravel", "language", "locale", "translation", "multilingual"],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Duli√ßi",
            "email": "info@akaunting.com",
            "homepage": "https://akaunting.com",
            "role": "Original Developer"
        },
        {
            "name": "FoxInnovs",
            "role": "Laravel 12 Fork Maintainer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/translation": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Akaunting\\Language\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akaunting\\Language\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akaunting\\Language\\Provider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}